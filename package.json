{
  "name": "restaurant-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "develop": "concurrently \"yarn:develop:frontend\" \"yarn:develop:backend\"",
    "build": "NODE_ENV=production next build && next export",
    "start": "cd out && http-server -p 3000 --cors",
    "lint": "run-s lint:*",
    "develop:frontend": "next dev",
    "develop:backend": "yarn --cwd 'backend' develop",
    "start:next": "next start",
    "lint:markdown": "markdownlint-cli2 .",
    "lint:js": "cd backend && eslint",
    "postinstall": "yarn --cwd 'backend'"
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.10.0",
    "axios": "^0.21.0",
    "dotenv": "^16.0.1",
    "graphql": "^15.3.0",
    "http-server": "^14.1.1",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^2.2.1",
    "next": "9.5.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "reactstrap": "^8.6.0"
  },
  "description": "Exercise05 is full Shopping Cart with Stripe Billing",
  "main": "index.js",
  "author": "jrw@mit.edu",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.2.1",
    "eslint": "^8.18.0",
    "markdownlint-cli2": "^0.4.0",
    "npm-run-all": "^4.1.5"
  }
}
